{
  "name": "jet-plan-ui",
  "version": "0.0.0",
  "browserslist": 4,
  "private": true,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/jet-plan-ui.cjs.js",
  "module": "./dist/jet-plan-ui.es.js",
  "unpkg": "./dist/jet-plan-ui.umd.js",
  "jsdelivr": "./dist/jet-plan-ui.umd.js",
  "exports": {
    ".": {
      "types": "./dist/packages/jet-plan-ui/index.d.ts",
      "module": "./dist/jet-plan-ui.es.js",
      "import": "./dist/jet-plan-ui.es.js",
      "require": "./dist/jet-plan-ui.cjs.js",
      "default": "./dist/jet-plan-ui.es.js"
    }
  },
  "scripts": {
    "devServer": "vite --port 5173 --host",
    "preview": "vite preview --port 5173 --host",
    "test:unit": "vitest --environment jsdom",
    "test:e2e:dev": "start-server-and-test devServer 5173 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview 5173 'cypress run --e2e'",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "gen": "node ./script/genNewComp/index.js",
    "build-only": "vite build",
    "build:lib": "run-p type-check && vite build --config ./buildLib.config.ts",
    "build:doc": "run-p type-check build-only&& node script/copyDir.js"
  },
  "peerDependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "highlight.js": "^11.7.0",
    "pinia": "^2.0.21",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "dependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "highlight.js": "^11.7.0",
    "pinia": "^2.0.21",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@babel/types": "^7.20.5",
    "@rollup/pluginutils": "^4.2.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.56",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitest/ui": "^0.25.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.11",
    "babel-core": "^6.26.3",
    "core-js": "^3.8.3",
    "cypress": "^10.9.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "fp-ts": "^2.12.1",
    "handlebars": "^4.7.7",
    "happy-dom": "^6.0.4",
    "inferred-types": "^0.22.0",
    "inquirer": "^8.2.4",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.0",
    "prettier": "^2.7.1",
    "rollup": "^2.77.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.7.4",
    "vite": "^3.0.9",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-inspect": "^0.7.9",
    "vite-plugin-md": "^0.20.4",
    "vitest": "^0.23.0",
    "vue-tsc": "^1.0.11"
  },
  "workspaces": [
    "./packages/jet-plan-ui",
    "./packages/jet-plan-ui-theme"
  ]
}
